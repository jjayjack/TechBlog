<div class="container userLogin">
    <div class="login user-dash">
        <h1 class="headLabel">
            User Login
        </h1>
        <form class="form login-form user-password">
            <div class="form-group userlog">
                <label for="user-login">
                    <h3>Username</h3>
                </label>
                <input class="form-input" type="text" id="user-login" />
            </div>
            <div class="form-group userpass">
                <label for="password-login">
                    <h3>Password</h3>
                </label>
                <input class="form-input" type="password" id="password-login" />
            </div>
            <div class="button">
                <div class="form-group">
                    <button class="btn btn-primary loginBtn" type="submit">login</button>
                </div>
                <button type="button" class="btn btn-secondary signupBtn" id="signup">sign up</button>
            </div>
        </form>
    </div>
</div>
<script src="/js/login.js"></script>